<body>
<form action="/display-discount">
    <div class="form-group">
        <label>Product Description:</label>
        <input type="name" name="productDescription" class="form-control" placeholder="Enter product description" >
    </div>
    <div class="form-group">
        <label>List Price:</label>
        <input type="number" name="price" class="form-control" placeholder="Enter list price">
    </div>
    <div class="form-group">
        <label>Discount Percent:</label>
        <input type="number" name="discount" class="form-control" placeholder="Enter discount percent">
    </div>
    <button type="submit" class="btn btn-primary">Calculate Discount</button>
</form>
</body>
